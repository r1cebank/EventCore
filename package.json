    "react": "^0.14.8",
    "react-native": "^0.24.0",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.0",
