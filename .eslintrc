{
  "root": true,
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": 1,
    "strict": 0,
    "new-cap": 0,
    "no-undef": 2,
    "id-length": 0,
    "func-names": 0,
    "no-console": 1,
    "key-spacing": 0,
    "padded-blocks": 0,
    "no-multi-spaces": 0,
    "no-const-assign": 2,
    "no-param-reassign": 0,
    "import/no-unresolved": 1,
    "arrow-body-style": 0,
    "global-require": 0,
    "indent": [2, 4, {"SwitchCase": 1}],
    "react/prefer-stateless-function": 0,
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/sort-comp": 0,
    "react/jsx-boolean-value": 0,
    "react/jsx-closing-bracket-location": 0,
    "comma-dangle": [
      1,
      "never"
    ]
  },
  "ecmaFeatures": {
    "generators": true,
    "jsx": true
  },
  "envs": {
    "node": true,
    "commonjs": true,
    "browser": false
  }
}
