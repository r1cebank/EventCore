{
  "root": true,
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "max-len": 1,
    "strict": 0,
    "new-cap": 0,
    "no-undef": 2,
    "id-length": 0,
    "func-names": 0,
    "no-console": 1,
    "key-spacing": 0,
    "padded-blocks": 0,
    "no-multi-spaces": 0,
    "no-const-assign": 2,
    "no-param-reassign": 0,
    "indent": [2, 4],
    "comma-dangle": [
      1,
      "never"
    ]
  },
  "ecmaFeatures": {
    "generators": true,
    "jsx": true
  },
  "envs": {
    "node": true,
    "commonjs": true,
    "browser": false
  }
}
